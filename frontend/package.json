{
  "name": "phaser-parcel",
  "version": "1.0.0",
  "description": "A Phaser 3 game template using Parcel bundler",
  "scripts": {
    "build": "rm -f dist/* && parcel build index.html",
    "postbuild": "koji-vcc pwa",
    "prestart": "koji-vcc watch &",
    "start": "parcel serve index.html --port 8080"
  },
  "keywords": [
    "phaser",
    "phaser3",
    "phaser-boilerplate"
  ],
  "author": "samme",
  "license": "ISC",
  "dependencies": {
    "@withkoji/vcc": "^1.1.16",
    "config": "^3.3.1",
    "matter": "^0.2.0",
    "parcel": "1.12.3",
    "phaser": "^3.20.1",
    "phaser3-rex-plugins": "^1.1.14",
    "services": "0.0.3"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "parcel": "^1.12.3"
  }
}
